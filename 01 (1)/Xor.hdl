/**
 * Exclusive-or gate:
 * if ((a and Not(b)) or (Not(a) and b)) out = 1, else out = 0
 */
CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
 //   Or
  //  Not
    Not(in=a , out=outA );
    Not(in=b, out=outB );
    And(a=a , b=outB , out=Aout );
    And(a=outA , b=b , out=Bout );
    Or(a=Aout , b=Bout , out=out );
    
}